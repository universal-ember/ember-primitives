    "ember-source": "^6.10.1",
    "ember-template-lint": "^7.9.3",
    "ember-welcome-page": "^8.0.5",
    "eslint": "^9.39.2",
    "file-loader": "^6.2.0",
    "globby": "^16.1.0",
