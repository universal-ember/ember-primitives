  },
  "engines": {
    "node": ">= 20"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "type": "app",
    "version": 2
  },
  "imports": {
    "#src/*": "./app/*",
    "#docs/*": "./app/templates/*"
  },
  "exports": {
    "./tests/*": "./tests/*",
    "./*": "./app/*"
  }
