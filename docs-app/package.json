    "@tailwindcss/typography": "^0.5.15",
    "@types/babel__core": "^7.20.5",
    "@types/babel__standalone": "^7.1.7",
    "@types/eslint__js": "^9.14.0",
    "@types/qunit": "^2.19.10",
    "@types/rsvp": "^4.0.9",
    "autoprefixer": "^10.4.24",
