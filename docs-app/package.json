{
  "name": "docs-app",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "description": "Small description for docs-app goes here",
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build:dev": "vite build",
    "lint": "concurrently 'pnpm:lint:*(!fix)' --names 'lint:'",
    "lint:fix": "concurrently 'pnpm:lint:*:fix' --names 'fix:' && pnpm format",
    "lint:hbs": "ember-template-lint . --no-error-on-unmatched-pattern",
    "lint:hbs:fix": "ember-template-lint . --fix --no-error-on-unmatched-pattern",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "ember-tsc --noEmit",
    "lint:prettier": "prettier --check .",
    "format": "prettier --write .",
    "start": "pnpm vite",
    "dev": "vite",
    "sync": "echo 'pnpm is syncing injected deps. (this is configured in .npmrc)'",
    "test:ember": "vite build --mode development && testem ci --port 0"
  },
  "dependencies": {
    "@embroider/macros": "^1.19.7",
    "@shikijs/rehype": "^3.22.0",
    "@universal-ember/docs-support": "workspace:*",
    "assert": "^2.0.0",
    "change-case": "^5.4.4",
    "decorator-transforms": "2.3.1",
    "ember-element-helper": "^0.8.8",
    "ember-focus-trap": "^1.1.0",
    "ember-mobile-menu": "^6.0.0",
    "ember-modifier": "^4.3.0",
    "ember-primitives": "workspace:^",
    "ember-repl": "^7.3.5",
    "ember-strict-application-resolver": "^0.1.1",
    "form-data-utils": "^0.6.0",
    "hash": "^0.2.1",
    "highlight.js": "^11.8.0",
    "highlightjs-glimmer": "^2.2.1",
    "kolay": "^5.0.1",
    "limber-ui": "^4.1.1",
    "lorem-ipsum": "^2.0.8",
    "nvp.ui": "^0.5.3",
    "path-browserify": "^1.0.1",
    "reactiveweb": "^1.9.1",
    "shiki": "^3.22.0",
    "should-handle-link": "^1.2.2",
    "tracked-built-ins": "^4.1.0",
    "unist-util-visit": "^5.1.0",
    "which-heading-do-i-need": "workspace:*"
  },
  "dependenciesMeta": {
    "ember-primitives": {
      "injected": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/plugin-transform-runtime": "^7.29.0",
    "@babel/plugin-transform-typescript": "^7.28.6",
    "@babel/runtime": "^7.28.6",
    "@ember/app-tsconfig": "^1.0.0",
    "@ember/optional-features": "^3.0.0",
    "@ember/string": "^4.0.0",
