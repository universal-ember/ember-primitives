{
  "name": "which-heading-do-i-need",
  "version": "0.2.5",
  "description": "The default blueprint for Embroider v2 addons.",
  "type": "module",
  "keywords": [
    "html",
    "section",
    "heading",
    "section-heading",
    "a11y",
    "headless"
  ],
  "repository": "https://github.com/universal-ember/ember-primitives",
  "license": "MIT",
  "author": "NullVoxPopuli",
  "exports": {
    ".": {
      "types": "./declarations/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "declarations",
    "dist"
  ],
  "scripts": {
    "dev": "rollup --config --watch --no-watch.clearScreen",
    "build": "rollup --config",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefixColors auto && pnpm run format",
    "lint:format": "prettier . --check",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "tsc --noEmit",
    "prepack": "rollup --config"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-transform-typescript": "^7.28.5",
    "@babel/runtime": "^7.28.4",
    "@ember/library-tsconfig": "^2.0.0",
    "@embroider/addon-dev": "^8.2.0",
    "@nullvoxpopuli/eslint-configs": "^5.5.0",
    "@rollup/plugin-babel": "^6.1.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "prettier": "^3.7.4",
    "rollup": "^4.55.1",
    "typescript": "^5.9.3"
  }
}
