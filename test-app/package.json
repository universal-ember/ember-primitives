{
  "name": "test-app",
  "version": "0.0.0",
  "private": true,
  "description": "Test app for ember-primitives addon",
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "lint": "concurrently 'pnpm:lint:*(!fix)' --names 'lint:'",
    "lint:fix": "concurrently 'pnpm:lint:*:fix' --names 'fix:' && pnpm format",
    "lint:hbs": "ember-template-lint . --no-error-on-unmatched-pattern",
    "lint:hbs:fix": "ember-template-lint . --fix --no-error-on-unmatched-pattern",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "ember-tsc --noEmit",
    "lint:prettier": "prettier --check .",
    "format": "prettier --write .",
    "start": "pnpm vite",
    "dev": "vite",
    "sync": "echo 'pnpm is syncing injected deps. (this is configured in .npmrc)'",
    "test:ember": "vite build --mode development && testem ci --port 0"
  },
  "dependencies": {
    "decorator-transforms": "2.3.1",
    "ember-primitives": "workspace:^",
    "ember-resources": "^7.0.7",
    "ember-route-template": "^1.0.3",
    "qunit-assertions-extra": "^1.0.3",
    "reactiveweb": "^1.9.1",
    "which-heading-do-i-need": "workspace:*"
  },
  "dependenciesMeta": {
    "ember-primitives": {
      "injected": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/plugin-transform-runtime": "^7.29.0",
    "@babel/plugin-transform-typescript": "^7.28.6",
    "@babel/runtime": "^7.28.6",
    "@ember/app-tsconfig": "^1.0.0",
    "@ember/optional-features": "^3.0.0",
    "@ember/string": "^4.0.0",
    "@ember/test-helpers": "^5.4.1",
    "@ember/test-waiters": "^4.1.0",
    "@embroider/core": "^4.4.3",
    "@embroider/macros": "^1.19.7",
    "@embroider/vite": "^1.5.2",
    "@floating-ui/dom": "^1.7.5",
    "@glimmer/component": "^2.0.0",
    "@glint/ember-tsc": "^1.1.1",
    "@glint/template": "1.7.4",
    "@glint/tsserver-plugin": "^2.1.0",
    "@nullvoxpopuli/eslint-configs": "^5.5.0",
    "@rollup/plugin-babel": "^6.1.0",
    "@testing-library/dom": "^10.4.1",
    "@types/eslint__js": "^9.14.0",
    "@types/qunit": "^2.19.10",
    "@types/rsvp": "^4.0.9",
    "babel-plugin-ember-template-compilation": "^4.0.0",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^9.2.1",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-load-initializers": "^3.0.1",
    "ember-modifier": "^4.3.0",
    "ember-page-title": "^9.0.3",
    "ember-qunit": "^9.0.4",
    "ember-resolver": "^13.1.1",
    "ember-source": "^6.10.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^7.9.3",
    "ember-try": "^4.0.0",
    "eslint": "^9.39.2",
    "globals": "^17.3.0",
    "loader.js": "^4.7.0",
    "prettier": "^3.8.1",
    "prettier-plugin-ember-template-tag": "^2.1.3",
    "qunit": "^2.24.3",
    "qunit-dom": "^3.5.0",
    "qunit-theme-ember": "^1.0.0",
    "read-package-up": "^12.0.0",
    "stylelint": "^17.2.0",
    "stylelint-config-standard": "^40.0.0",
    "stylelint-prettier": "^5.0.0",
    "testem": "^3.17.0",
    "tracked-built-ins": "^4.1.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  },
  "engines": {
    "node": ">= 20"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "type": "app",
    "version": 2
  },
  "exports": {
    "./tests/*": "./tests/*",
    "./*": "./app/*"
  }
}
