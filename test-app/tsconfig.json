{
  "extends": "@ember/app-tsconfig",
  "include": ["app", "tests", "types"],
  "compilerOptions": {
    "lib": "es2022",
    "skipLibCheck": true,
    "paths": {
      "test-app/tests/*": ["./tests/*"],
      "test-app/*": ["./app/*"],
      "*": ["./types/*"]
    },
    "types": ["ember-source/types", "@embroider/core/virtual", "@glint/ember-tsc/types"]
  }
}
